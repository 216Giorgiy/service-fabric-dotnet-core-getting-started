<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>CounterClient Console Application</Description>
    <Authors> </Authors>
    <TargetFramework>netcoreapp1.0</TargetFramework>
    <AssemblyName>CounterActorTestClient</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>CounterActorTestClient</PackageId>
    <RuntimeFrameworkVersion>1.0.4</RuntimeFrameworkVersion>
